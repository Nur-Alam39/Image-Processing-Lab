   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "image=cv2.imread(\"BSMRSTU.jpg\")\n",
    "h,w=image.shape[:2]\n",
    "\n",
    "x1,y1=int(h*.25),int(w*.25)\n",
    "x2,y2=int(h*.75),int(w*.75)\n",
    "\n",
    "croped_image=image[x1:x2,y1:y2]\n",
    "\n",
    "cv2.imshow('Orginal',image)\n",
    "cv2.imshow('Croped Image',croped_image)\n",
    "\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
